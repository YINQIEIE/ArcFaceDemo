<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arcsoft.sdk_demo">
    <!-- camera -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- sdcard -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- wifi internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.arcsoft.sdk_demo.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.arcsoft.sdk_demo.PermissionActiivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.arcsoft.sdk_demo.RegisterActivity" />
        <activity android:name="com.arcsoft.sdk_demo.DetecterActivity" />
        <activity android:name="com.arcsoft.sdk_demo.MainActivity" />
        <activity android:name=".CameraFragmentTestActivity" />
    </application>

</manifest>